




<div data-role="collapsible-set">
  <% @speaker.each do |s| %>
      <div data-role="collapsible" data-mini="true">
        <h6> <%= s.name %></h6>
        <div class="fbinfo">
          <table>
            <tr>
              <td style="vertical-align: top;"><%= image_tag s.avatar.url, :style => 'max-height:100px;max-width:100px;margin:10px;' %></td>
            </tr>
            <tr>
              <td><div style="font-size: 0.8em;font-weight: normal"><%= raw s.speaker_details %></div></td>
            </tr>
          </table>
        </div>
      </div>
  <% end %>


</div>










