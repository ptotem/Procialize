<%= content_for :page_title do %>
    Products
<% end %>




<% @events.each do |e| %>
    <div data-role="collapsible" data-collapsed="false" data-collapsed-icon="arrow-r"  data-expanded-icon="arrow-d">
      <h6><%= e.name %></h6>
      <ul data-role="listview">
        <% @tra= Track.find_all_by_event_id(e.id) %>
        <% @tra.each do |f| %>
            <li>
              <a href="/events/<%= f.id %>">
                <%= f.name %>
              </a>
            </li>
        <% end %>
      </ul>
    </div>
    <br>
    <br>
<% end %>














