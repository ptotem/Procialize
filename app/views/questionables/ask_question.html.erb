<%= content_for :page_title do %>
    Questions
<% end %>





<div data-role="content" id="categoriesContent">
  <ul data-role="listview" data-theme="d" data-dividertheme="c" data-counttheme="b" style="font-size: 0.8em;font-family: helveticaneue;">
    <% Questionable.where(:approved => true).reverse.each do |e| %>
        <% if !e.quest_name.nil?   %>
            <li><%= e.quest_name %>  asked by <%= User.find(e.user_id).name %> at <%= Track.find(e.track_id).name %></li>
        <% end %>
    <% end %>
  </ul>
</div>

