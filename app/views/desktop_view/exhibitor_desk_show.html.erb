<%#= content_for :jj do %>
    <!--<script src="/assets/testing/dashboard/javascripts/application_all.js" type="text/javascript"></script>-->
<%# end %>



<div class="container-fluid">
    <div class="row-fluid">
        <div class="span7">
            <div class="row-fluid">
              <div class="span12">
                <%= render :partial => "list" %>
              </div>
            </div>

        <!----------------------Start of EXHIBITOR SHOW ------------------------------>

            <div class="black-box tex" id="delegate_profile_of_user">

              <ul class="recent-comments">
                <li class="separator">
                  <% if !@exhibitor.exhi_logo.blank? %><%= image_tag @exhibitor.exhi_logo.url, :style => "float:left; border-radius:5px;margin-right:10px;max-width:80px" %><% end %>

                  <h4 style="margin-top: 0px;"><%= @exhi_name.titlecase %>
                    <table>
                      <tr><td><% if !@exhi_industry.blank? %><small><strong style="color: #c0c0c0;">Industry: </strong><%= @exhi_industry %> </small><% end %></td></tr>

                      <tr><td><% if !@exhi_description.blank? %><small><strong style="color: #c0c0c0;">Description: </strong><%= raw @exhi_description %></small><br><% end %></td></tr>

                      <tr><td><% if !@exhi_stall.blank? %><small><strong style="color: #c0c0c0;">Stall No: </strong><%= @exhi_stall %></small><br><% end %></td></tr>

                      <tr><td><% if !@exhi_contact_no.blank? %><small><strong style="color: #c0c0c0;">Contact No: </strong><%= @exhi_contact_no %></small><br><% end %></td></tr>

                      <tr><td><% if !@exhibitor.brouchers.blank? %><small><a target="_blank"  href="<%=@exhibitor.brouchers.url%>" data-ajax=false data-role=button data-mini=true>Company Brochure<i class=" icon-download  "></i></a></small><% end %></td></tr>

                      <tr><td><% if !@exhibitor.broucher2.blank? %><small><a target="_blank"  href="<%=@exhibitor.broucher2.url%>" data-ajax=false data-role=button data-mini=true>Product Brochure<i class=" icon-download  "></i></a></small><% end %></td></tr>

                      <tr>
                        <td>
                          <small>
                            <% @user_name.each do |u|  %>
                                <table cellspacing="10">
                                  <% if !u.name.nil? %>
                                      <strong style="color: #c0c0c0;">Represented by: </strong>
                                       <tr>
                                          <td align="position"valign="position"><%= u.name %></td>
                                          <td align="position"valign="position"><a style="font-size: 0.8em;margin-left: 10px;" class="button gray" href="<%= desktop_focussed_meeting_path(u) %>">Request Meeting</a></td>
                                          <td align="position"valign="position"><a style="font-size: 0.8em;margin-left: 10px;" class="button gray" href="<%= desktop_focussed_message_path(u) %>/?reply=true">Send Message</a></td>
                                      </tr>
                                    <% end %>
                                </table>
                            <% end %>
                          </small>
                        </td>
                      </tr>
                    </table>
                  </h4>
                </li>
              </ul>
            </div>
        <!----------------------End of EXHIBITOR SHOW------------------------------>
        </div>

        <div class="span5">
          <div class="row-fluid">
            <div class="span12">
              <%= render :partial => "event_wall" %>
            </div>
          </div>
        </div>

        <div class="span5">
          <div class="row-fluid">
            <div class="span12">
              <%= render :partial => "splash_zone" %>
            </div>
          </div>
        </div>

    </div>

  <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
              m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-41279198-1', '127.0.0.1:3000');
      ga('send', 'pageview');

  </script>

</div>





