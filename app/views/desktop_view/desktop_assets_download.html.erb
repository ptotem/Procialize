<%#= content_for :jj do %>
<!--<script src="/assets/testing/dashboard/javascripts/application_all.js" type="text/javascript"></script>-->
<%# end %>





<div class="container-fluid">
    <div class="row-fluid">
        <div class="span7">
              <div class="row-fluid">
                <div class="span12">
                  <%= render :partial => "list" %>
                </div>
              </div>

      <!----------------------Start of Assets Download------------------------------>

              <div class="black-box tex" >
                <div class="tab-header">Assets Download</div>
                    <ul class="recent-comments">
                        <li class="separator">
                            <div>
                                 <div class="box" style="position:relative;padding: 10px;color: black;">
                                    <table class="table data-table" id="example">
                                        <thead>
                                            <tr>
                                             <th>Event Name</th>
                                             <th>Downloads</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% @assets_downloads.each do |e| %>
                                                <%if e.downloadable.url!='/avatars/original/missing.png'%>
                                                    <% @event_id= e.track_id%>
                                                <tr class="gradeA even"  id="<%= e %>">
                                                  <td><%= @event_name=Track.find(@event_id).name %></td>
                                                  <td style="border-left: 1px solid #DDDDDD"><a target="_blank" href="<%=e.downloadable.url%>"><img src="/assets/procdownload.png"></a>&nbsp;&nbsp;<%= e.downloadable.original_filename %></td>
                                                </tr>
                                            <% end %>
                                            <% end %>
                                        </tbody>
                                    </table>
                                 </div>
                            </div>
                        </li>
                    </ul>
              </div>

      <!----------------------End of Assets Download------------------------------>
        </div>

        <div class="span5">
          <div class="row-fluid">
            <div class="span12">
              <%= render :partial => "event_wall" %>
            </div>
          </div>
        </div>

        <div class="span5">
          <div class="row-fluid">
            <div class="span12">
              <%= render :partial => "splash_zone" %>
            </div>
          </div>
        </div>
  </div>

